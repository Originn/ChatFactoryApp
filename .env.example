# Vercel Configuration
VERCEL_API_TOKEN=your-vercel-token

# AI API Keys
OPENAI_API_KEY=sk-your-openai-key
MISTRAL_API_KEY=your-mistral-key

# Cleanup Configuration
# Set to 'true' to enable more aggressive cleanup that scans ALL files and attempts bucket deletion
FORCE_AGGRESSIVE_CLEANUP=false
ANTHROPIC_API_KEY=your-anthropic-key
COHERE_API_KEY=your-cohere-key

# Pinecone Configuration
PINECONE_API_KEY=your-pinecone-key

# CHM Converter Service
CHM_CONVERTER_URL=https://chm-converter-931513743743.us-central1.run.app

# PDF Converter Service
