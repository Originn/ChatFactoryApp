USE_REUSABLE_FIREBASE_PROJECT=false
NEXT_PUBLIC_USE_REUSABLE_FIREBASE_PROJECT=false

# Pre-created Google Cloud project ID (only this is needed - the code will set up Firebase automatically)
REUSABLE_FIREBASE_PROJECT_ID=my-dev-chatbot-firebase

